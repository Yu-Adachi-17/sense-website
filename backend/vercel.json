{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://sense-website-production.up.railway.app/api/:path*"
      }
    ]
  }
  